{
  "name": "alphavantage-ts",
  "version": "1.1.2",
  "description": "A simple interface to the Alpha Vantage API written in Typescript.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist/**/*"],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "prettier --print-width 120 --single-quote --write \"./**/**.js\"",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "alpha vantage",
    "vantage",
    "alpha",
    "alphavantage",
    "stocks",
    "stock market",
    "api",
    "typescript"
  ],
  "authors": [
    "Zack Urben <zackurben@gmail.com> (http://zackurben.com)",
    "Adam King <adamjking3@gmail.com> (https://github.com/adamjking3)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zackurben/alphavantage"
  },
  "homepage": "https://github.com/adamjking3/alphavantage-ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "cross-fetch": "^3.0.4"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "delay": "^4.3.0",
    "jest": "^22.4.4",
    "prettier": "^1.19.1"
  },
  "jest": {
    "collectCoverage": true,
    "bail": false,
    "verbose": true,
    "testMatch": [
      "**/test/**.js"
    ],
    "testEnvironment": "node"
  }
}
